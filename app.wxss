/* 图标 */
@font-face {
  font-family: "iconfont"; /* Project id 2526336 */
  src: url('/class/iconfont.woff2?t=1620186263504') format('woff2'),
       url('/class/iconfont.woff?t=1620186263504') format('woff'),
       url('/class/iconfont.ttf?t=1620186263504') format('truetype');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

